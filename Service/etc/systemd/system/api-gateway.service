[Unit]
Description=Quantum API Gateway Agent
After=network.target

[Service]
Type=simple
WorkingDirectory=/root/quantumdev-open
ExecStart=/root/quantumdev-open/venv/bin/python3 /root/quantumdev-open/agents/api_gateway.py
Restart=always
RestartSec=5
Environment="PYTHONUNBUFFERED=1"

[Install]
WantedBy=multi-user.target
