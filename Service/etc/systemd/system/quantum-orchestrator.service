
[Unit]
Description=Quantum Orchestrator Agent
After=network.target

[Service]
Type=simple
WorkingDirectory=/root/quantumdev-open
ExecStart=/root/quantumdev-open/venv/bin/python3 /root/quantumdev-open/agents/orchestrator.py
Restart=always
RestartSec=5
Environment="PYTHONUNBUFFERED=1"
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target
